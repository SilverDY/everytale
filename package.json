  "scripts": {
    "analyze": "yarn-or-npm build:with-stats && yarn-or-npm start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.ts",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.ts/client.prod.ts --json > build/client/static/bundle-stats.json",
    "lint": "concurrently \"yarn-or-npm lint:js\" \"yarn-or-npm lint:css\"",
    "lint:js": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:css": "stylelint './src/**/*.css'",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/start.ts",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "start-client": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/start.ts --client-only",
    "start-client:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "transpile": "yarn-or-npm babel -d build/transpiled ./src --extensions .es6,.js,.es,.jsx,.mjs,.ts,.tsx --ignore **/*.d.ts",
    "tsc": "tsc --noEmit"
  },
